{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
    "description": "Ether trading range",
  
     "title": {
      "text": "Ether: November",
      "subtitle":"High-low trading range, 8h time periods. Source: coinapi.io",
      "subtitleFontStyle":"italic",
      "subtitleFontSize":10,
      "anchor": "start",
      "color": "#000000",
      "subtitleColor":"#000000"},
  
    "config": {
      "background":"#ffffff",
      "view": {
        "stroke": "transparent"}},
   
    
    "data": {
      "url": "https://rest.coinapi.io/v1/ohlcv/ETH/USD/history?period_id=8HRS&time_start=2021-11-01T00:00:00&limit=100&include_empty_items=false&apikey=FCB2BB27-5458-483B-88CE-926C357E0606",
      "format": {"type": "json"}},
    
      "XYZ_data": {
        "url": "https://raw.githubusercontent.com/RDeconomist/RDeconomist.github.io/main/charts/crypto/data_ohlcv_ETH.json",
        "format": {"type": "json"}},
    
  
  
    "height": 290,
    "width": 290,
    
  
    "mark": {
      "type": "area",
      "point": false,
      "interpolate": "linear",
      "color":"hotpink"
    },
  
    
    "encoding": {
      "x": {
        "field": "time_period_start",
        "type": "temporal",
        "axis": {
          "title": null,
          "grid": false,
          "ticks": true,
          "domainColor":"dimgrey",
          "labelColor":"#000000",
          "gridColor":"dimgrey"
        }
      },
  
      "y": {
        "field": "price_low",
        "type": "quantitative",
        "title": "",
        "scale":{"domain":[2000, 6000]},
        "axis": {
          "grid": false,
          "domainColor":"dimgrey",
          "labelColor":"#000000",
          "XYZlabelSeparation":20,	
          "tickColor":"black",
          "gridColor":"dimgrey",
          "format":"$.2s"}},
  
      "y2": {"field": "price_high"},
  
      "tooltip":[
        {"field":"time_period_start", "title": "Date", "type":"temporal" },
        {"field":"price_low", "title": "Low", "format":"$,.2d" },
        {"field":"price_high", "title": "High", "format":"$,.2d" }
  
      ]
       
       }
  }